%{
//
// Finite state machine for the Performance test between SMC and boost StateCharts
//
#if __GNUC__ >4 || ( __GNUC__==4 && __GNUC_MINOR__ > 1)
#  pragma GCC diagnostic ignored "-Wall"
#  pragma GCC diagnostic ignored "-Wunused"
#  pragma GCC diagnostic ignored "-Weffc++"
#  pragma GCC diagnostic ignored "-Wunused-parameter"
#endif
#
#include <string.h>
#include <cstring>
%}

%include <tests/sdpa/PerformanceTestFSMEvent.hpp>

%package sdpa::tests
%class FSMPerformanceTest
%header test_FSMPerformance.hpp

%start PerformanceTest::S0
%map PerformanceTest

%%
S0
{
  T(e: const ::sdpa::tests::PerformanceTestFSMEvent&)
    S1
    {
      do_s0_s1(e);
    }
  F(e: const ::sdpa::tests::PerformanceTestFSMEvent&)
    S1
    {
      do_throw_exception();
    }
}

S1
{
  T(e: const ::sdpa::tests::PerformanceTestFSMEvent&)
    S0
    {
      do_s1_s0(e);
    }
  F(e: const ::sdpa::tests::PerformanceTestFSMEvent&)
    S1
    {
      do_throw_exception();
    }
}

%%
