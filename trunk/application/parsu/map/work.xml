<defun name="work">
  <include-structs href="types.xml"/>
  <in name="config" type="config" place="config"/>
  <in name="part" type="part_in_store" place="in"/>
  <out name="part" type="part_in_store" place="out"/>
  <out name="credit" type="control" place="credit"/>
  <net>
    <place name="in" type="part_in_store"/>
    <place name="tmp" type="part_in_store"/>
    <place name="out" type="part_in_store"/>
    <place name="config" type="config"/>
    <place name="credit" type="control"/>

    <transition name="work">
      <defun>
        <in name="config" type="config"/>
        <in name="in" type="part_in_store"/>
        <out name="out" type="part_in_store"/>
        <module name="filter_trace" function="execW"/>
      </defun>
      <connect-in port="config" place="config"/>
      <connect-in port="in" place="in"/>
      <connect-out port="out" place="tmp"/>
    </transition>

    <transition name="generate_credit">
      <defun>
        <in name="x" type="part_in_store"/>
        <out name="x" type="part_in_store"/>
        <out name="credit" type="control"/>
        <expression>${credit} := []</expression>
      </defun>
      <connect-in port="x" place="tmp"/>
      <connect-out port="x" place="out"/>
      <connect-out port="credit" place="credit"/>
    </transition>
  </net>
</defun>
