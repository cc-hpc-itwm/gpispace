<structs>
  <struct name="config">
    <struct name="num">
      <field name="store" type="long"/>
      <field name="part" type="long"/>
      <field name="write_credit" type="long"/>
    </struct>

    <struct name="input">
      <field name="file" type="string"/>
      <field name="type" type="string"/>
    </struct>

    <struct name="output">
      <field name="file" type="string"/>
      <field name="type" type="string"/>
    </struct>

    <struct name="data">
      <field name="size" type="long"/>
    </struct>

    <struct name="bunchbuffer">
      <field name="size" type="long"/>
    </struct>

    <struct name="handle">
      <field name="data" type="long"/>
      <field name="scratch" type="long"/>
    </struct>

    <struct name="trace_detect">
      <field name="size_in_bytes" type="long"/>
      <field name="number" type="long"/>
    </struct>

    <struct name="tune">
      <field name="memsize" type="long"/>
      <field name="trace_per_bunch" type="long"/>
      <field name="slots_per_node" type="long"/>
    </struct>

    <struct name="param">
      <struct name="tpow">
        <field name="tpow" type="double"/>
      </struct>
      <struct name="clip">
        <field name="c" type="double"/>
      </struct>
      <struct name="trap">
        <field name="t" type="double"/>
      </struct>
      <struct name="bandpass">
        <field name="frequ1" type="double"/>
        <field name="frequ2" type="double"/>
        <field name="frequ3" type="double"/>
        <field name="frequ4" type="double"/>
      </struct>
    </struct>
    <struct name="exec">
      <field name="su" type="string"/>
    </struct>
  </struct>

  <struct name="part_in_store">
    <struct name="id">
      <field name="part" type="long"/>
      <field name="store" type="long"/>
    </struct>
  </struct>
</structs>
