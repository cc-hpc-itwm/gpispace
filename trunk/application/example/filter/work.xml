<defun name="work">
  <include-structs href="types.xml"/>
  <in name="config" type="config" place="config"/>
  <in name="part" type="part_in_store" place="in"/>
  <out name="part" type="part_in_store" place="out"/>
  <net>
    <place name="in" type="part_in_store"/>
    <place name="out" type="part_in_store"/>
    <place name="config" type="config"/>

    <transition name="work">
      <defun>
        <in name="config" type="config"/>
        <in name="in" type="part_in_store"/>
        <out name="out" type="part_in_store"/>
        <module name="filter_trace" function="clip"/>
      </defun>
      <connect-in port="config" place="config"/>
      <connect-in port="in" place="in"/>
      <connect-out port="out" place="out"/>
    </transition>
  </net>
</defun>
