<defun>

  <in name="config" type="config" place="config"/>
  <out name="store_bunch" type="store_bunch" place="store_bunch"/>
  <out name="store_volume" type="store_volume" place="store_volume"/>
  <out name="number_store_volume" type="long" place="number_store_volume"/>
  <out name="number_store_bunch" type="long" place="number_store_bunch"/>

  <net>

    <place name="config" type="config"/>

    <place name="store_bunch" type="store_bunch"/>
    <place name="store_volume" type="store_volume"/>

    <place name="number_store_bunch" type="long"/>
    <place name="number_store_volume" type="long"/>

    <place name="config_bunch" type="config"/>
    <place name="config_volume" type="config"/>

    <include-template href="dup.xml"/>
    <specialize name="dup_config" use="dup">
      <type-map replace="T" with="config"/>
    </specialize>

    <transition name="dup_config" inline="true">
      <use name="dup_config"/>
      <connect-in port="in" place="config"/>
      <connect-out port="one" place="config_bunch"/>
      <connect-out port="two" place="config_volume"/>
    </transition>

    <transition name="generate_bunch" inline="true">
      <include-function href="store/bunch.xml"/>
      <connect-in port="config" place="config_bunch"/>
      <connect-out port="store_bunch" place="store_bunch"/>
      <connect-out port="number" place="number_store_bunch"/>
    </transition>

    <transition name="generate_volume" inline="true">
      <include-function href="store/volume.xml"/>
      <connect-in port="config" place="config_volume"/>
      <connect-out port="store_volume" place="store_volume"/>
      <connect-out port="number" place="number_store_volume"/>
    </transition>

  </net>

</defun>
