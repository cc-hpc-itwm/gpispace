<template name="read">
  <in name="filename" type="string"/>
  <out name="x" type="T"/>
  <module name="ufbmig" function="read (filename, x)">
    <cinclude href="fhglog/fhglog.hpp"/>
    <cinclude href="util/rw.hpp"/>
    <code><![CDATA[
      LOG (INFO, "read from file " << filename);

      ::ufbmig::util::serialize::read (filename, x);
    ]]></code>
  </module>
</template>
