<defun name="pnete">
  <in name="in" type="long" place="in"/>
  <out name="done" type="control" place="done"/>
  <net>
    <place name="in" type="long">
      <properties name="fhg">
        <properties name="pnete">
          <properties name="position">
            <property key="x" value="100"/>
            <property key="y" value="100"/>
          </properties>
        </properties>
      </properties>
    </place>
    <place name="d" type="long"/>
    <place name="done" type="control"/>
    <transition name="double">
      <properties name="fhg">
        <properties name="pnete">
          <properties name="position">
            <property key="x" value="314.1"/>
            <property key="y" value="271.8"/>
          </properties>
        </properties>
      </properties>
      <defun>
        <in name="in" type="long"/>
        <out name="out" type="long"/>
        <expression>
          ${out} := 2 * ${in}
        </expression>
      </defun>
      <connect-in port="in" place="in"/>
      <connect-out port="out" place="d"/>
    </transition>

    <transition name="eat">
      <properties name="fhg">
        <properties name="pnete">
          <properties name="position">
            <property key="x" value="200"/>
            <property key="y" value="-100"/>
          </properties>
        </properties>
      </properties>
      <defun>
        <in name="in" type="long"/>
        <out name="done" type="control"/>
        <expression>
          ${done} := []
        </expression>
      </defun>
      <connect-in port="in" place="d"/>
      <connect-out port="done" place="done"/>
    </transition>
  </net>
</defun>
