<workflow xmlns="http://www.gridworkflow.org/gworkflowdl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xsi:schemaLocation="http://www.gridworkflow.org/gworkflowdl http://www.gridworkflow.org/kwfgrid/src/xsd/gworkflowdl_2_0.xsd" ID="No_ID">
    <!-- $Id: gworkflowdl-concatenateIt.xml 2582 2009-06-05 15:10:12Z andreas.hoheisel@first.fraunhofer.de $ -->
    <description>concatenate three files</description>

    <place ID="d25">
        <token><data><file>file:///usr/local/gwes/cat/d25.dat</file></data></token>
    </place>
    <place ID="d26">
        <token><data><file>file:///usr/local/gwes/cat/d26.dat</file></data></token>
    </place>
    <place ID="d27">
        <token><data><file>file:///usr/local/gwes/cat/d27.dat</file></data></token>
    </place>
    <place ID="d25-26"/>
    <place ID="d25-27"/>
    <place ID="stderr"/>
    <place ID="exitcode"/>

    <transition ID="cat1">
        <description>concatenate two files</description>
        <inputPlace placeID="d25" edgeExpression="input1"/>
        <inputPlace placeID="d26" edgeExpression="input2"/>
        <outputPlace placeID="d25-26" edgeExpression="stdout"/>
 		<operation>
      		<oc:operationClass xmlns:oc="http://www.gridworkflow.org/gworkflowdl/operationclass" name="cat">
        		<oc:operationCandidate type="cli" operationName="cat.sh" resourceName="/usr/local/gwes/cat/cat.shXXX" selected="true"/>
     	 	</oc:operationClass>
    	</operation>
    </transition>

    <transition ID="cat2">
        <description>concatenate two files</description>
        <inputPlace placeID="d25-26" edgeExpression="input1"/>
        <inputPlace placeID="d27" edgeExpression="input2"/>
        <outputPlace placeID="d25-27" edgeExpression="stdout"/>
        <outputPlace placeID="stderr" edgeExpression="stderr"/>
        <outputPlace placeID="exitcode" edgeExpression="exitcode"/>
 		<operation>
      		<oc:operationClass xmlns:oc="http://www.gridworkflow.org/gworkflowdl/operationclass" name="cat">
        		<oc:operationCandidate type="cli" operationName="cat.sh" resourceName="/usr/local/gwes/cat/cat.shXXX" selected="true"/>
     	 	</oc:operationClass>
    	</operation>
    </transition>

</workflow>