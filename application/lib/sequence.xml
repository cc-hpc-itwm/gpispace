<defun name="sequence">
  <in name="amount" type="long" place="N"/>
  <out name="out" type="long" place="id"/>

  <net>
    <place name="N" type="long"/>
    <place name="id" type="long"/>
    <place name="left" type="long"><token><value>0L</value></token></place>
    <transition name="ntom" inline="true">
      <include-function href="sequence/ntom.xpnet"/>
      <connect-in port="right" place="N"/>
      <connect-read port="left" place="left"/>
      <connect-out port="out" place="id"/>
    </transition>
  </net>
</defun>
