<defun name="z_eq_x_plus_y">
  <in name="x" type="long" place="a"/>
  <in name="y" type="long" place="b"/>
  <out name="z" type="long" place="s"/>
  <net>
    <place name="a" type="long"/>
    <place name="b" type="long"/>
    <place name="s" type="long"/>
    <transition name="sum">
      <include-function href="sum.xml"/>
      <connect-in port="a" place="a"/>
      <connect-in port="b" place="b"/>
      <connect-out port="s" place="s"/>
    </transition>
  </net>
</defun>
