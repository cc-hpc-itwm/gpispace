<defun name="join">
  <struct name="O_t"/>
  <struct name="o_t"/>
  <struct name="N_t"/>

  <in name="N" type="N_t" place="N"/>
  <in name="o" type="o_t" place="o"/>
  <out name="O" type="O_t" place="O"/>

  <net>
    <struct name="state_t"/>

    <place name="N" type="N_t"/>
    <place name="o" type="o_t"/>
    <place name="O" type="O_t"/>
    <place name="state" type="state_t"/>

    <transition name="init">
      <defun>
        <in name="N" type="N_t"/>
        <out name="state" type="state_t"/>
        <expression></expression>
      </defun>
      <connect-in port="N" place="N"/>
      <connect-out port="state" place="state"/>
    </transition>

    <transition name="join">
      <defun>
        <in name="state" type="state_t"/>
        <out name="state" type="state_t"/>
        <in name="o" type="o_t"/>
        <expression></expression>
        <condition>${state}</condition>
      </defun>
      <connect-in port="state" place="state"/>
      <connect-out port="state" place="state"/>
      <connect-in port="o" place="o"/>
    </transition>

    <transition name="break">
      <defun>
        <in name="state" type="state_t"/>
        <out name="O" type="O_t"/>
        <expression></expression>
        <condition>!${state}</condition>
      </defun>
      <connect-in port="state" place="state"/>
      <connect-out port="O" place="O"/>
    </transition>
  </net>
</defun>
