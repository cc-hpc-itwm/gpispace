<template name="print_T">
  <template-parameter type="T"/>
  <defun>
    <in name="x" type="T"/>
    <out name="printed" type="string"/>
    <module name="print" function="printed print (x)">
      <cinclude href="sstream"/>
      <code><![CDATA[
      std::ostringstream oss;
      oss << x;
      return oss.str();
      ]]></code>
    </module>
  </defun>
</template>
